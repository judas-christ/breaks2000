{
  "name": "breaks2000",
  "version": "2.1.2",
  "description": "Simple responsive elements script",
  "devDependencies": {
    "rimraf": "^2.4.1",
    "rollup": "^0.41.4",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-replace": "^1.1.1",
    "uglifyjs": "^2.4.10"
  },
  "scripts": {
    "bundle": "rollup -c",
    "minify": "uglifyjs dist/breaks2000.js -o dist/breaks2000.min.js -c --lift-vars -mt --drop_console --comments \"/\\/*!/\"",
    "build": "rimraf dist && mkdir dist && npm run bundle && npm run minify"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/judas-christ/breaks2000.git"
  },
  "license": "MIT"
}
